<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_map"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.couriertrack.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/home2"
        android:name="com.example.couriertrack.Fragment.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_resi"
            app:destination="@id/resi" />
    </fragment>
    <fragment
        android:id="@+id/resi"
        android:name="com.example.couriertrack.Resi"
        android:label="fragment_resi"
        tools:layout="@layout/fragment_resi" >
        <action
            android:id="@+id/action_resi_to_home2"
            app:destination="@id/home2" />
    </fragment>
</navigation>